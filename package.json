{
    "name": "sketch",
    "version": "0.0.1",
    "description": "Three.js sketch materials",
    "keywords": [
        "webgl",
        "boilerplate",
        "three.js",
        "es6",
        "webpack"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack -p",
        "build-dev": "cross-env NODE_ENV=development webpack --debug --output-pathinfo",
        "watch": "cross-env NODE_ENV=development webpack --debug --output-pathinfo -w",
        "test": "cross-env NODE_ENV=development BABEL_ENV=test mocha --require babel-register -R spec test/"
    },
    "dependencies": {
        "raf": "^3.4.0",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "three": "^0.95.0",
        "three-obj-loader": "^1.1.3",
        "three-orbit-controls": "^82.1.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-espower": "^2.4.0",
        "babel-plugin-glslify": "glslify/babel-plugin-glslify",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "brfs": "^2.0.0",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "file-loader": "^1.1.11",
        "glsl-noise": "^0.0.0",
        "glslify": "^6.2.1",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.1",
        "raw-loader": "^0.5.1",
        "shelljs": "^0.8.2",
        "webpack": "^4.16.5",
        "webpack-cli": "^3.1.0"
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
